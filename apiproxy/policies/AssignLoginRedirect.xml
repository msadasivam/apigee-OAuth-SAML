<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignLoginRedirect">
    <Set>
        <Headers>
            <Header name="Location">{samlinfo.deswithurl}</Header>
            <Header name="Cache-Control">no-cache, no-store, must-revalidate</Header>
            <Header name="Pragma">no-cache</Header>
            <Header name="Expires">0</Header>
        </Headers>
        <StatusCode>302</StatusCode>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>